{
  "AnalysisServices": {
    "Server": "asazure://<region>.asazure.windows.net/<servername>",
    "Database": "<database_name>"
  },
  "AzureOpenAI": {
    "Endpoint": "https://<your-resource-name>.openai.azure.com/",
    "ApiKey": "<your-api-key>",
    "DeploymentName": "<deployment-name>"
  },
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
